{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "cosmosDB",
      "databaseName": "Devices",
      "collectionName": "Devices",
      "connectionStringSetting": "CosmosDbConnectionString",
      "partitionKey": "/Devices",
      "COSMOS_API_URL": "COSMOS_API_URL",
      "COSMOS_API_KEY": "COSMOS_API_KEY"
    }
  ]
}
